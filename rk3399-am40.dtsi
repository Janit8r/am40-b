&i2s2 {
    status = "okay";
    assigned-clocks = <&cru SCLK_I2S2_DIV>;
    assigned-clock-parents = <&cru PLL_GPLL>;
    rockchip,i2s-broken-burst-len;
    rockchip,playback-channels = <0>;
    rockchip,capture-channels = <128>;
    #sound-dai-cells = <0>;
};

&rng {
    status = "okay";
};

&gpu {
    status = "okay";
    mali-supply = <&vdd_gpu>;
};

&vopl {
    status = "okay";
    assigned-clocks = <&cru DCLK_VOP1_DIV>;
    assigned-clock-parents = <&cru PLL_CPLL>;
};

&vopl_mmu {
    status = "okay";
};

&vopb {
    status = "okay";
    assigned-clocks = <&cru DCLK_VOP0_DIV>;
    assigned-clock-parents = <&cru PLL_VPLL>;
};

&vopb_mmu {
    status = "okay";
};

&hdmi {
    pinctrl-names = "default";
    #address-cells = <1>;
    #size-cells = <0>;
    #sound-dai-cells = <0>;

    /* DDC-I2C timing specification */
    ddc-i2c-scl-high-time-ns = <9625>;
    ddc-i2c-scl-low-time-ns = <10000>;
    rockchip,hdmi_video_source = <0>;
    
    /* PHY configuration table with proper formatting */
    rockchip,phy-table = <
        74250000    0x8009    0x0004    0x0272
        165000000   0x802b    0x0004    0x0209
        297000000   0x8039    0x0004    0x028d
        594000000   0x8039    0x0000    0x019d
        0           0x0000    0x0000    0x0000
    >;
    status = "okay";
};

&hdmi_in_vopl {
    status = "disabled";
};

&display_subsystem {
    status = "okay";
    logo-memory-region = <&drm_logo>;
};

&route_hdmi {
    status = "okay";
};

&pinctrl {
    pinctrl-names = "default","sleep";
    pinctrl-0 = <&hdmi_en &sdmmc_en &pwr_en>;
    pinctrl-1 = <&sdmmc_sleep &pwr_sleep>;
    
    gmac {
        rgmii_sleep_pins: rgmii-sleep-pins {
            rockchip,pins = <3 15 0 &pcfg_output_low>;
        };
    };
    
    sdmmc {
        sdmmc_en:sdmmc-en {
            rockchip,pins = <0 1 0 &pcfg_pull_up>;
        };

        sdmmc_sleep:sdmmc-sleep {
            rockchip,pins = <0 1 0 &pcfg_output_low>;
        };
    };
    
    hdmi {
        hdmi_en:hdmi-en {
            rockchip,pins = 
            <1 13 0 &pcfg_output_high>,
            <4 28 0 &pcfg_output_high>;
        };

        hdmi_sleep:hdmi-sleep {
            rockchip,pins = <4 29 0 &pcfg_output_low>;
        };
    };
    
    isp {
        cif_clkout:cif-clkout {
            rockchip,pins = <2 11 3 &pcfg_pull_none>;
        };

        isp_dvp_d0d7:isp-dvp-d0d7 {
            rockchip,pins = 
            <2 0 3 &pcfg_pull_none>,
            <2 1 3 &pcfg_pull_none>,
            <2 2 3 &pcfg_pull_none>,
            <2 3 3 &pcfg_pull_none>,
            <2 4 3 &pcfg_pull_none>,
            <2 5 3 &pcfg_pull_none>,
            <2 6 3 &pcfg_pull_none>,
            <2 7 3 &pcfg_pull_none>,
            <2 8 3 &pcfg_pull_none>,
            <2 9 3 &pcfg_pull_none>,
            <2 10 3 &pcfg_pull_none>;
        };

        isp_shutter:isp-shutter {
            rockchip,pins = 
            <1 1 1 &pcfg_pull_none>,
            <1 0 1 &pcfg_pull_none>;
        };

        isp_flash_trigger:isp-flash-trigger {
            rockchip,pins = <1 3 1 &pcfg_pull_none>;
        };

        isp_prelight:isp-prelight {
            rockchip,pins = <1 4 1 &pcfg_pull_none>;
        };
        
        isp_flash_trigger_as_gpio: isp-flash-trigger-as-gpio {
            rockchip,pins = <1 3 0 &pcfg_pull_none>;
        };
    };
    
    sdio-pwrseq {
        wifi_enable_h:wifi-enable-h {
            rockchip,pins = <0 10 0 &pcfg_pull_none>;
        };
    };
    
    wireless-bluetooth {
        uart0_gpios:uart0-gpios {
            rockchip,pins = <2 19 0 &pcfg_pull_none>;
        };
    };
    
    usb2 {
        host_vbus_drv:host-vbus-drv {
            rockchip,pins = <1 3 0 &pcfg_pull_none>;
        };
    };
    
    pmic {
        pmic_int_l:pmic-int-l {
            rockchip,pins = <1 21 0 &pcfg_pull_up>;
        };

        vsel1_gpio:vsel1-gpio {
            rockchip,pins = <1 17 0 &pcfg_pull_down>;
        };

        vsel2_gpio:vsel2-gpio {
            rockchip,pins = <1 14 0 &pcfg_pull_down>;
        };
    };
    
    gpio-leds {
        leds_gpio: leds-gpio {
            rockchip,pins = <4 18 0 &pcfg_pull_none>;
        };
    };
    
    am40-gpio {
        pwr_en:pwr-en {
            rockchip,pins = 
            <0 10 0 &pcfg_output_high>,
            <0 11 0 &pcfg_output_high>,
            <1 4 0 &pcfg_output_high>;
        };

        pwr_sleep:pwr-sleep {
            rockchip,pins = 
            <2 28 0 &pcfg_output_low>,
            <4 29 0 &pcfg_output_low>;
        };
    };
    
    eth_phy {
        eth_phy_pwr:eth-phy-pwr {
            rockchip,pins = <1 13 0 &pcfg_output_high>;
        };
    };

    pt7c_irq {
        pt7c4563_irq:pt7c4563-irq {
            rockchip,pins = <0 6 0 &pcfg_pull_up>;
        };
    };
};

&rockchip_suspend {
    status = "okay";
    rockchip,sleep-debug-en = <0>;
    rockchip,sleep-mode-config = <
        (0
        | RKPM_SLP_ARMPD
        | RKPM_SLP_PERILPPD
        | RKPM_SLP_DDR_RET
        | RKPM_SLP_PLLPD
        | RKPM_SLP_CENTER_PD
        | RKPM_SLP_AP_PWROFF
        )
    >;
    rockchip,wakeup-config = <
        (0
        | RKPM_GPIO_WKUP_EN
        | RKPM_PWM_WKUP_EN
        )
    >;
    rockchip,pwm-regulator-config = <
        (0
        | PWM2_REGULATOR_EN
        )
    >;
    rockchip,power-ctrl =
        <&gpio1 17 GPIO_ACTIVE_HIGH>,
        <&gpio1 14 GPIO_ACTIVE_HIGH>;
};
